{
  "compilerOptions": {
    "allowJs": true,
    "module": "system",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "target": "ES5",
    "declaration": false,
    "noResolve": false
  },
  "exclude": [
    "node_modules",
    "bower_components",
    "typings/main",
    "typings/main.d.ts"
  ]
}